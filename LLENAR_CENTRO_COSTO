create or replace
PROCEDURE LLENAR_CENTRO_COSTO AS 
CURSOR C1 IS SELECT DISTINCT(JEF_INFA_CTR_ID_CENTRO) ID, CC.CTR_DESCRIPCION DES FROM SDS_FORMATOS_FMO_DATA_TABLE FTO, CENTRO_COSTOS CC WHERE FTO.JEF_INFA_CTR_ID_CENTRO=CC.CTR_ID_CENTRO;
BEGIN
  FOR I IN C1 LOOP
    INSERT INTO D_CENTRO_DE_COSTO VALUES(SECCOSTO.NEXTVAL, I.ID, I.DES);
  END LOOP;
  NULL;
END LLENAR_CENTRO_COSTO;
